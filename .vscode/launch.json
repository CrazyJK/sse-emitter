    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch SseController",
            "request": "launch",
            "mainClass": "com.hs.common.notificator.SseController",
            "projectName": "notificator-sse-emitter"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch NotificatorApplication",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "mainClass": "com.hs.common.notificator.NotificatorApplication",
            "projectName": "notificator-sse-emitter"
        }
    ]
}